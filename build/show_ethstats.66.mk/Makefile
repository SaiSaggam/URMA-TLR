#
# Makefile to compile eth_stats
#

include ../makefile.vars

COMPO_DIR := $(COMMANDS_BASE)/show/eth_stats

build:
	@make -C $(COMPO_DIR) ${PREFIX}

install:
	@make -C $(COMPO_DIR) install ${PREFIX}

clean:
	@make -C $(COMPO_DIR) clean ${PREFIX}

distclean:
	@make -C $(COMPO_DIR) distclean

copying: 

