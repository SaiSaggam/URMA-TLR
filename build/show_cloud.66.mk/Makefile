#
# Makefile to compile show process for cloud status and statistics
#

include ../makefile.vars

COMPO_DIR := $(COMMANDS_BASE)/show/cloud

build:
	@make -C $(COMPO_DIR) ${PREFIX}

install:
	@make -C $(COMPO_DIR) install ${PREFIX}

clean:
	@make -C $(COMPO_DIR) clean ${PREFIX}

distclean:
	@make -C $(COMPO_DIR) distclean

copying: 

